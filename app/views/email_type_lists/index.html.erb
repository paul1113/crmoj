<h1>Listing email_type_lists</h1>

<table>
  <thead>
    <tr>
      <th>Uuid</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @email_type_lists.each do |email_type_list| %>
      <tr>
        <td><%= email_type_list.uuid %></td>
        <td><%= email_type_list.name %></td>
        <td><%= link_to 'Show', email_type_list %></td>
        <td><%= link_to 'Edit', edit_email_type_list_path(email_type_list) %></td>
        <td><%= link_to 'Destroy', email_type_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Email type list', new_email_type_list_path %>
